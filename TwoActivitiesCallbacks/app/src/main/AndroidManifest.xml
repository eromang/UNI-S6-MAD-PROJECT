<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lu.uni.mad.twoactivitiescallbacks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TwoActivities">
        <activity android:name="lu.uni.mad.twoactivitiescallbacks.SecondActivity"
            android:label = "@string/activity2_name"
            android:parentActivityName="lu.uni.mad.twoactivitiescallbacks.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="lu.uni.mad.twoactivitiescallbacks.MainActivity" />
        </activity>
        <activity android:name="lu.uni.mad.twoactivitiescallbacks.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>